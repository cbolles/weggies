<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SetPreferences">

    <CheckBox
        android:id="@+id/antibioticFree"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="286dp"
        android:layout_marginBottom="8dp"
        android:fontFamily="@font/advent_pro_medium"
        android:text="No Antibiotics"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/cornFree"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/hormoneFree"
        android:onClick="onCheckboxClicked"/>

    <CheckBox
        android:id="@+id/cornFree"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="281dp"
        android:layout_marginBottom="5dp"
        android:fontFamily="@font/advent_pro_medium"
        android:text="No Corn Syrup"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/irradiated"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/antibioticFree"
        android:onClick="onCheckboxClicked"/>

    <CheckBox
        android:id="@+id/wild"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="301dp"
        android:layout_marginBottom="5dp"
        android:fontFamily="@font/advent_pro_medium"
        android:onClick="onCheckboxClicked"
        android:text="Wild Caught"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/certifiedHumane"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fair" />

    <CheckBox
        android:id="@+id/certifiedHumane"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="270dp"
        android:layout_marginBottom="6dp"
        android:fontFamily="@font/advent_pro_medium"
        android:text="Certified Humane"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/hormoneFree"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/wild"
        android:onClick="onCheckboxClicked"/>

    <CheckBox
        android:id="@+id/irradiated"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="317dp"
        android:layout_marginBottom="5dp"
        android:fontFamily="@font/advent_pro_medium"
        android:onClick="onCheckboxClicked"
        android:text="Non-Irradiated"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/msgFree"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cornFree" />

    <CheckBox
        android:id="@+id/lactovovegitarian"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="328dp"
        android:layout_marginBottom="10dp"
        android:fontFamily="@font/advent_pro_medium"
        android:onClick="onCheckboxClicked"
        android:text="Lactovovegitarian"
        app:layout_constraintBottom_toTopOf="@+id/fair"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        app:layout_constraintVertical_chainStyle="spread_inside" />

    <CheckBox
        android:id="@+id/msgFree"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="318dp"
        android:layout_marginBottom="23dp"
        android:fontFamily="@font/advent_pro_medium"
        android:text="No MSG"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/irradiated"
        android:onClick="onCheckboxClicked"/>

    <CheckBox
        android:id="@+id/fair"
        android:layout_width="90dp"
        android:layout_height="35dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="305dp"
        android:layout_marginBottom="8dp"
        android:fontFamily="@font/advent_pro_medium"
        android:onClick="onCheckboxClicked"
        android:text="Fair Trade"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/wild"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lactovovegitarian" />

    <CheckBox
        android:id="@+id/hormoneFree"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="17dp"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="248dp"
        android:layout_marginBottom="11dp"
        android:fontFamily="@font/advent_pro_medium"
        android:text="No Added Hormones"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/antibioticFree"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/certifiedHumane"
        android:onClick="onCheckboxClicked" />


    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="424dp"
        android:layout_height="286dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/pref1" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="379dp"
        android:layout_height="73dp"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/chalkpref" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="195dp"
        android:layout_height="32dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="8dp"
        android:fontFamily="@font/cabin_sketch"
        android:text="Select From Below"
        android:textAlignment="center"
        android:textColor="@android:color/background_light"
        android:textSize="24sp"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5" />

    <android.support.constraint.Barrier
        android:id="@+id/barrier"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="left"
        tools:layout_editor_absoluteX="205dp"
        tools:layout_editor_absoluteY="447dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="88dp"
        android:layout_height="395dp"
        android:layout_marginStart="115dp"
        android:layout_marginEnd="44dp"
        android:layout_marginBottom="24dp"
        android:background="@color/colorPrimaryDark"
        android:backgroundTint="@color/colorPrimaryDark"
        android:backgroundTintMode="src_atop"
        android:fontFamily="@font/cabin_sketch"
        android:onClick="goToNext"
        android:text=">>"
        android:textAlignment="center"
        android:textColor="@android:color/background_light"
        android:textSize="60sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/hormoneFree" />

</android.support.constraint.ConstraintLayout>